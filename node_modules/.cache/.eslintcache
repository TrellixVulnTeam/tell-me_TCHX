\n          >\n            Create\n          </Button>\n        </Link>\n      </form>\n     <Link to='/' className=\"logo\"><h2 className=\"logo\">Tell <span>me</span></h2></Link>\n    </div>\n  );\n}\n\nexport default CreatePost;\n","/Users/admin/Documents/mystudy/react/tell-me-react/tell-me-react/src/components/UpdatePost.jsx",[],"/Users/admin/Documents/mystudy/react/tell-me-react/tell-me-react/src/components/UserPage.jsx",[],"/Users/admin/Documents/mystudy/react/tell-me-react/tell-me-react/src/components/Button.jsx",[],"/Users/admin/Documents/mystudy/react/tell-me-react/tell-me-react/src/components/Input.jsx",[],"/Users/admin/Documents/mystudy/react/tell-me-react/tell-me-react/src/components/Pagination.jsx",["114"],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":3,"column":22,"nodeType":"121","messageId":"122","endLine":3,"endColumn":30},{"ruleId":"119","severity":1,"message":"123","line":1,"column":28,"nodeType":"121","messageId":"122","endLine":1,"endColumn":40},{"ruleId":"119","severity":1,"message":"124","line":3,"column":10,"nodeType":"121","messageId":"122","endLine":3,"endColumn":21},{"ruleId":"125","severity":1,"message":"126","line":36,"column":13,"nodeType":"127","endLine":36,"endColumn":124},{"ruleId":"119","severity":1,"message":"124","line":3,"column":23,"nodeType":"121","messageId":"122","endLine":3,"endColumn":34},{"ruleId":"119","severity":1,"message":"128","line":5,"column":10,"nodeType":"121","messageId":"122","endLine":5,"endColumn":21},{"ruleId":"119","severity":1,"message":"129","line":11,"column":10,"nodeType":"121","messageId":"122","endLine":11,"endColumn":15},{"ruleId":"119","severity":1,"message":"130","line":13,"column":9,"nodeType":"121","messageId":"122","endLine":13,"endColumn":17},{"ruleId":"119","severity":1,"message":"131","line":2,"column":17,"nodeType":"121","messageId":"122","endLine":2,"endColumn":23},{"ruleId":"119","severity":1,"message":"132","line